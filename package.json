    "eslint-plugin-standard": "5.0.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.2",
    "next-sitemap": "^1.6.157",
    "pinst": "^2.1.6",
    "prettier": "^2.3.2",
    "rustywind": "0.12.2",
    "tailwindcss": "2.2.7",
    "typescript": "^4.3.5"
  }
